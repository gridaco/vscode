{
  "name": "grida-vscode",
  "displayName": "Grida",
  "description": "Grida's official extension for vscode users",
  "version": "0.4.0",
  "publisher": "grida",
  "homepage": "https://grida.co/vscode",
  "repository": "https://github.com/gridaco/vscode-extension",
  "preview": true,
  "icon": "branding/logo-256-no-edit.png",
  "engines": {
    "vscode": "^1.62.0"
  },
  "categories": [
    "Other",
    "Visualization"
  ],
  "keywords": [
    "Grida",
    "Figma",
    "Sketch",
    "React",
    "Design to code",
    "Typescript",
    "Frontend tools"
  ],
  "main": "./dist/extension.js",
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "vscode:packagejson": "node ./scripts/copy-vscode-attributes.js",
    "download-api": "vscode-dts dev",
    "postdownload-api": "vscode-dts main",
    "postinstall": "yarn run download-api",
    "clean": "rimraf dist",
    "compile": "webpack --config ./build/node-extension.webpack.config.js",
    "watch": "webpack --watch --config ./build/node-extension.webpack.config.js",
    "package": "webpack --mode production --devtool hidden-source-map --config ./build/node-extension.webpack.config.js",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "yarn run test-compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@design-sdk/figma-remote-types": "^0.0.5",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^16.11.6",
    "@types/node-fetch": "^2.5.7",
    "@types/uuid": "8.0.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^7.15.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "vscode-test": "^1.6.1",
    "vscode-dts": "^0.3.3",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "@base-sdk-fp/accounts": "^0.0.3",
    "@base-sdk-fp/auth": "^0.1.0-8",
    "@code-features/assets": "0.0.0",
    "@code-features/component": "0.0.0",
    "@code-features/flags": "0.0.0",
    "@coli.codes/core-syntax-kind": "0.0.0",
